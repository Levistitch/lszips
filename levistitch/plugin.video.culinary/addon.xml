<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.culinary" name="Culinary Delights" version="1.0" provider-name="Levi">
    <requires>
        <import addon="xbmc.python" version="2.19.0" />
        <import addon="script.module.nanscrapers" version="0.5.5" />
        <import addon="script.module.python.koding.aio" version="0.9.6.6" />
        <import addon="script.module.beautifulsoup" version="3.2.1" />
        <import addon="script.module.requests" version="2.9.1" />
        <import addon="script.module.addon.common" version="2.0.0" />
        <import addon="script.module.xmltodict" version="0.9.0" />
        <!--<import addon="script.module.web-pdb" version="1.0"/>-->
        <import addon="script.module.urlresolver" version="1.2.0" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="[login|startup]" />
    <extension point="xbmc.addon.metadata">
      <summary lang="en">Cooking Delights</summary>
      <description lang="en">Addon for cooking recipes and videos</description>
      <disclaimer lang="en">The author of this addon do not host any of the content which is found.  All content is found freely on the internet</disclaimer>
      <platform>all</platform>
        <website></website>
        <video_guide></video_guide>
    </extension>
</addon>
