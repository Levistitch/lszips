<addon id="plugin.program.ls"
       name="LS Wizard" 
       version="1.0.1"
       provider-name="Levistitch">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>program executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
		<summary lang="en">Welcome to the LS Wizard</summary>
		<description lang="en">This Software is designed to keep your KODI device up to date and running smoothly</description>
		<disclaimer lang="en">For help and support ... Twitter @Levistitch</disclaimer>
		<platform>all</platform>
	</extension>
</addon>
	
<addon id="plugin.video.bgnews" name="[COLORaqua]News 13[/COLOR]" version="1.0.1" provider-name="Levistitch, Dazzler">
	<requires>
		<import addon="xbmc.python" version="2.19.0" />
		<import addon="script.module.nanscrapers" version="0.5.5" />
        <import addon="script.module.python.koding.aio" version="0.0.1"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Wbko</summary>
		<description lang="en">Wbko News 13.  Live when live is on.  Replays when not live.  The developer is in no way, shape, or form affiliated or associated with XBMC or Kodi.</description>
		<platform>all</platform>
		<forum>For support @Levistitch</forum>
		<website></website>
		<news></news>
		<genre>News</genre>
		<video_guide></video_guide>
		<video_preview></video_preview>
	</extension>
</addon>

